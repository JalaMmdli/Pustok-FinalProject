@model ServiceCreateDto

<form enctype="multipart/form-data" method="post">
    <div class="form-group">
        <label asp-for="File"></label>
        <input class="form-control" asp-for="File" placeholder="Enter icon">
        <span asp-validation-for="File" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Title">Title</label>
        <input class="form-control" asp-for="Title" placeholder="Enter Title">
        <span asp-validation-for="Title" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Description">Description</label>
        <input class="form-control" asp-for="Description" placeholder="Enter Description">
        <span asp-validation-for="Description" class="text-danger"></span>
    </div>
    
    <div asp-validation-summary="ModelOnly" class="text-danger">  </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>
